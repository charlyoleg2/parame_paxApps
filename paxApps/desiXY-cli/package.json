{
	"name": "desiXY-cli",
	"description": "The cli-frontend for desiXY",
	"version": "0.5.2",
	"private": true,
	"repository": {
		"type": "git",
		"url": "git+https://github.com/charlyoleg2/parame_paxApps.git"
	},
	"homepage": "https://charlyoleg2.github.io/parame_paxApps/",
	"author": "charlyoleg",
	"license": "ISC",
	"keywords": [
		"parametrix",
		"3D",
		"CAD"
	],
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/desiXY-cli.d.ts",
			"default": "./dist/desiXY-cli.js"
		}
	},
	"bin": "./dist/desiXY-cli.js",
	"files": [
		"dist/",
		"!dist/**/*.d.ts",
		"!dist/**/*.map",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"tsup": {
		"entry": [
			"src/desiXY-cli.ts"
		],
		"format": "esm",
		"splitting": false,
		"dts": false,
		"sourcemap": false,
		"clean": true
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"check": "tsc --noEmit",
		"pretty": "prettier --check .",
		"format": "prettier --write .",
		"lint": "eslint .",
		"test:unit": "vitest",
		"test:unit:once": "vitest --run",
		"test:bats": "bats test/test1.bats",
		"ci": "run-s check build pretty lint test:unit:once test:bats",
		"run": "dist/desiXY-cli.js",
		"clean": "shx rm -fr build dist node_modules output src/designList.ts src/versions.json"
	},
	"dependencies": {
		"geomcli": "^0.5.47",
		"geometrix": "^0.5.44"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"bats": "^1.11.0",
		"eslint": "^8.57.0",
		"eslint-config-prettier": "^9.1.0",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.3.2",
		"shx": "^0.3.4",
		"tsup": "^8.1.0",
		"typescript": "^5.4.5",
		"vitest": "^1.6.0"
	}
}
